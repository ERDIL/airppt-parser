{
  "name": "airppt-parser",
  "version": "1.0.8",
  "description": "Parses Powerpoint (PPTX) to Standardized JSON Objects",
  "main": "js/main.js",
  "types": "js/main.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "TS_NODE_PROJECT=./tsconfig.json ./node_modules/mocha/bin/_mocha --require ts-node/register -u tdd --timeout 999999  --colors ./ts/**/*.spec.ts"
  },
  "repository": {
    "type": "airppt-parser",
    "url": "https://github.com/airpptx/airppt-parser"
  },
  "keywords": [
    "airppt",
    "parser",
    "pptx",
    "powerpoint",
    "html",
    "webconvert",
    "json",
    "ppt"
  ],
  "author": "Raviteja Lingineni",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^10.12.9",
    "airppt-models": "^1.0.8",
    "jszip": "^3.1.5",
    "module-alias": "^2.1.0",
    "string-template": "^1.0.0",
    "xml2js-es6-promise": "^1.1.1"
  },
  "_moduleAliases": {
    "@helpers": "./js/helpers/",
    "@models": "./js/models/",
    "@generators": "./js/generators/",
    "@renderers": "./js/renderers/"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
